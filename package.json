    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "husky": "^6.0.0",
    "node-sass": "^5.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
